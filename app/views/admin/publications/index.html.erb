<h1>Publications</h1>
<%= button_to "Add Publication", new_admin_publication_path, method: "get" %>
<table class="table-borders">
  <% @publications.each do |publication| %>
    <tr>
      <td><%= link_to publication.title, admin_publication_path(publication) %></td>
      <td><%= link_to "Edit", edit_admin_publication_path(publication) %>&nbsp;&nbsp;<%= link_to "Delete", admin_publication_path(publication), method: :delete, data: { confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
</table>
